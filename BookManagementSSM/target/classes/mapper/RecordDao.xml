<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ddu.dao.RecordDao">
    <insert id="addRecord" parameterType="com.ddu.entity.Record">
        INSERT INTO record(frequency,hours,name,phoneNumber)
        VALUES
        (#{record.frequency},#{record.hours},#{record.name},#{record.phoneNumber})
    </insert>

</mapper>